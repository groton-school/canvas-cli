{
  "actions" : {
    "build" : {
      "enabled" : true,
      "postActions" : [
        {
          "action" : "build",
          "taskName" : "build",
          "type" : "runTask"
        }
      ]
    },
    "run" : {
      "enabled" : true,
      "script" : "source .env\ncanvas-import --stdoutLevel debug --no-ignoreErrors --duplicates update --no-topics --no-assignments \"$SNAPSHOT_PATH\""
    }
  },
  "buildBeforeRunning" : true,
  "openLogOnRun" : "start"
}
