<div>
    <h3>News</h3>
    <dl>
        <% content.Content.forEach(item => { %>
        <dt><%= item.Name %></dt>
        <dd><%= item.PublishDate %></dd>
        <% for (const photo of item.PhotoList || []) { %>
            <dd><img id="<%= photo.LargeFilenameUrl.id %>" src="<%= photo.LargeFilenameUrl.url %>" alt="<%= photo.LargeFilenameUrl.display_name %>" style="height: 2em;" /></dd>
        <% } %>
        <dd><%- item.BriefDescription %></dd>
        <dd><%- item.Description %></dd>
        <% }) %>
    </dl>
</div>
