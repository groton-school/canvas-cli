<div>
    <h3>Downloads</h3>
    <% if (Array.isArray(content.Content)) { %>
        <dl>
        <% for (const item of content.Content) { %>
            <dt><a class="instructure_file_link inline_disabled" title="<%= item.DownloadUrl.display_name %>" href="<%= item.DownloadUrl.url %>" target="_blank" rel="noopener noreferrer"><%= item.Description %></a></dt>
            <dd><%- item.LongDescription %></dd>
        <% } %>
        </dl>
    <% } else { %>
        <pre lang="json"><%= JSON.stringify(content, null, 2) %></pre>
    <% } %>
</div>
