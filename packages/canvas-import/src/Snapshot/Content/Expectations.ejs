<div>
    <h3>Expectations</h3>
    <% content.Content.forEach(item => { %>
    <% if (item.AttachmentQueryString) { %>
        <div><small><a class="instructure_file_link inline_disabled" title="<%= item.AttachmentQueryString.canvas.display_name %>" href="<%= item.AttachmentQueryString.canvas.url %>" target="_blank" rel="noopener noreferrer"><%= item.ShortDescription %></a></small></div>
        <% } %>
        <div><%- item.Description %></div>
    <% }); %>
</div>
