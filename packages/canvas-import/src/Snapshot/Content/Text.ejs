<% if (Array.isArray(content.Content)) { %>
<% for (const item of content.Content) { %>
    <div>
        <h3><%= item.Description %></h3>
        <div style="display: flex; flex-direction: row;">
            <% if (item.Photos) { %>
                <%- include('Canvas/ImageGallery', {images: item.Photos.map(p => ({...p.LargeFilenameUrl.canvas, caption: p.Caption})), course_id, style: 'height: 100%'}) %>
            <% } %>
            <div><%- item.LongText %></div>
        </div>
    </div>
<%} %>
<% } else { %>
<div><%- content.LongDescription %></div>
<% }%>
