<% if (Array.isArray(content.Content)) { %>
<% for (const item of content.Content) { %>
    <div>
        <h3><%= item.Description %></h3>
        <div style="display: flex; flex-direction: row;">
            <% if (item.Photos) {
                for(const photo of item.Photos) { %>
                <a href="photo." style="display: block;">
                    <div>
                        <img id="<%= photo.LargeFilenameUrl.id %>" src="<%= photo.LargeFilenameUrl.url %>" alt="<%= photo.LargeFilenameUrl.display_name %>" style="height: 3em;"/>
                    </div>
                    <div><%= photo.Caption %></div>
                </a>
            <% }} %>
            <div><%- item.LongText %></div>
        </div>
    </div>
<%} %>
<% } else { %>
<pre lang="json"><%= JSON.stringify(content, null, 2) %></pre>
<% }%>
