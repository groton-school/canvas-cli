<% content.Content.forEach(item => { %>
    <div>
        <h3><%= item.Description %></h3>
        <div style="display: flex; flex-direction: row;">
            <% if (item.Photos) {
                for(const photo of item.Photos) { %>
                <a href="photo." style="display: block;">
                    <div>
                        <img id="<%= photo.LargeFilenameUrl.id %>" src="<%= photo.LargeFilenameUrl.preview_url %>" alt="<%= photo.LargeFilenameUrl.display_name %>" height="100"/>
                    </div>
                    <div><%= photo.Caption %></div>
                </a>
            <% }} %>
            <div><%- item.LongText %></div>
        </div>
    </div>
<%}) %>
