<div>
    <h3>Links</h3>
        <dl>
    <% if (Array.isArray(content.Content)) { %>
            <% for (const item of content.Content) { %>
                <dt><a href="<%= item.Url %>"><%= item.ShortDescription %></a></dt>
                <dd><%- item.Description %></dd>
            <% }; %>
    <% } else { %>
        <dt><a href="<%= content.Url %>"><%= content.ShortDescription %></a></dt>
        <dd><%= content.LongDescription %></dd>
    <% } %>
        </dl>
</div>
